{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "functions": {
    "api/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/chat",
      "destination": "/api/chat"
    },
    {
      "source": "/api/enhanced-chat",
      "destination": "/api/enhanced-chat"
    },
    {
      "source": "/api/health",
      "destination": "/api/health"
    }
  ]
}